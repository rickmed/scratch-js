{
  "compilerOptions": {
      "strict": true,
      "moduleResolution": "NodeNext",
      "module": "NodeNext",
      "esModuleInterop": true,
      "isolatedModules": true,
      "target": "ES2020",
      "jsx": "react-jsx",
      "jsxImportSource": "react",
      "checkJs": true,
      "skipLibCheck": true,
      "sourceMap": true,
      "outDir": "./dist",
      "rootDir": ".",
      "declaration": true,
      "declarationMap": true,
      "moduleDetection": "force",
      "strictPropertyInitialization": true,
      "noImplicitReturns": true,
      "noUncheckedIndexedAccess": true,
  },
}