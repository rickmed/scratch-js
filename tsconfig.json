{
  "compilerOptions": {
     "strict": true,
     "module": "Node16",
     "moduleResolution": "Node16",
     "moduleDetection": "force",
     "target": "ES2022",
     "skipDefaultLibCheck": true,
     "sourceMap": true,
     "strictPropertyInitialization": true,
     "noUncheckedIndexedAccess": true
  },
}